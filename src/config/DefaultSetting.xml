<?xml version="1.0" encoding="UTF-8"?>


<MyJavaPianoSetting>

	<PianoSetting>
		
		<flat>0</flat><!-- 调 -->
		
		<v8_left>0</v8_left><!-- 8度 -->
		<v8_right>0</v8_right>
		
		<velocity_left>90</velocity_left><!-- 力度 -->
		<velocity_right>100</velocity_right>
	
		<delay_left>3</delay_left><!-- 延音 index-->
		<delay_right>3</delay_right><!-- 延音 -->
		
	</PianoSetting>
	



	<keysLayout>
	
